{{template "header" .}}
<div class="p-8 w-full">
    <h1 class="p-4 pb-8 text-3xl font-bold text-gray-800">
        {{.Title}}
    </h1>
    <div class="columns-4 gap-4 space-y-4">
        {{range .Images}}
            <div clas="h-min w-full">
                <a href="{{.}}">
                    <img class="w-full" src="{{.}}">
                </a>
            </div>
        {{end}}
    </div>
    </div>
    {{template "footer" .}}